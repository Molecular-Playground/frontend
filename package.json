    "debug": "~2.2.0",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "watchify": "^2.1.1"
  }
}
